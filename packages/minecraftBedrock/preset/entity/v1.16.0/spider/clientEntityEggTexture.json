				"walk": "animation.{{IDENTIFIER}}.walk"
			},
			"animation_controllers": [
				{ "move": "controller.animation.{{IDENTIFIER}}.move" }
			],
			"render_controllers": ["controller.render.{{IDENTIFIER}}"],
			"spawn_egg": {
				"texture": "{{PROJECT_PREFIX}}_{{IDENTIFIER}}_egg",
