{
	"name": "Template Pool",
	"icon": "mdi-library-shelves",
	"description": "Creates a blank template pool.",
	"category": "fileType.simpleFile",
	"requires": {
		"packTypes": [
			"behaviorPack"
		]
	},
	"additionalModels": {
		"PRESET_PATH": "worldgen/template_pools/"
	},
	"fields": [
		[
			"File Name",
			"FILE_NAME",
			{
				"validate": [
					"required",
					"alphanumeric",
					"lowercase"
				]
			}
		]
	],
	"createFiles": [
		[
			"templatePool.json",
			"{{PRESET_PATH}}{{FILE_NAME}}.json",
			{
				"inject": [
					"FILE_NAME",
					"PRESET_PATH"
				],
				"openFile": true,
				"packPath": "behaviorPack"
			}
		]
	]
}