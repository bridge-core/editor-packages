{
	"id": "fog",
	"icon": "mdi-weather-fog",
	"detect": {
		"packType": "resourcePack",
		"scope": "fogs/",
		"matcher": ["fogs/**/*.json", "fogs/*.json"],
		"fileContent": ["minecraft:fog"]
	},

	"target_version": "$project_target_version >= 1.16.100",

	"schema": "file:///data/packages/minecraftBedrock/schema/fog/main.json",
	"lightningCache": "fog.json",

	"documentation": {
		"baseUrl": "https://bedrock.dev/docs/stable/Fogs",
		"supportsQuerying": false
	},
	"highlighterConfiguration": {
		"variables": ["description", "distance", "volumetric"],
		"typeIdentifiers": ["format_version"]
	}
}
