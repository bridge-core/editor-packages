{
	"id": "gameTest",
	"type": "text",
	"icon": "mdi-test-tube",
	"detect": {
		"packType": "behaviorPack",
		"scope": "scripts/",
		"matcher": [
			"scripts/*.js",
			"scripts/**/*.js",
			"scripts/*.ts",
			"scripts/**/*.ts"
		]
	},

	"types": [
		[
			"types/gameTest/legacy/componentData.d.ts",
			{ "targetVersion": ["<=", "v1.17.30"] }
		],

		[
			"types/gameTest/legacy/v1.16.220.d.ts",
			{ "targetVersion": ["<=", "v1.16.220"] }
		],

		[
			"types/gameTest/legacy/v1.17.0.d.ts",
			{ "targetVersion": ["=", "v1.17.0"] }
		],

		[
			"types/gameTest/legacy/v1.17.10.d.ts",
			{ "targetVersion": ["=", "v1.17.10"] }
		],

		[
			"types/gameTest/legacy/v1.17.20.d.ts",
			{
				"targetVersion": ["=", "v1.17.20"]
			}
		],
		[
			"types/gameTest/legacy/v1.17.20.d.ts",
			{
				"targetVersion": ["=", "v1.17.30"]
			}
		],

		// From 1.17.40 we now use official types provided by Mojang
		// 1.17.40
		[
			"types/gameTest/v1.17.40.d.ts",
			{ "targetVersion": ["=", "v1.17.40"] }
		],
		[
			"types/gameTest/v1.17.40.d.ts",
			{ "targetVersion": ["=", "v1.18.0"] }
		],
		// 1.18.10
		[
			"types/gameTest/v1.18.10.d.ts",
			{ "targetVersion": [">=", "v1.18.10"] }
		]
	],

	"documentation": {
		"baseUrl": "https://docs.microsoft.com/en-us/minecraft/creator/documents/gametestgettingstarted",
		"supportsQuerying": false
	}
}
