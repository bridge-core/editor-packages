{
	"id": "feature",
	"icon": "mdi-flower",
	"detect": {
		"packType": "behaviorPack",
		"scope": "features/",
		"matcher": [
			"features/**/*",
			"features/*"
		],
		"fileExtensions": [
			".json"
		]
	},
	"schema": "file:///data/packages/minecraftBedrock/schema/feature/main.json",
	"lightningCache": "feature.json",
	"definitions": {
		"blockIdentifier": [
			{
				"from": "block",
				"match": "identifier"
			},
			{
				"from": "clientBlock",
				"match": "identifier"
			}
		]
	},
	"documentation": {
		"baseUrl": "https://bedrock.dev/docs/stable/Features"
	},
	"highlighterConfiguration": {
		"variables": [
			"description"
		],
		"typeIdentifiers": [
			"format_version"
		]
	}
}