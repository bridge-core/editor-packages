{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Client Biome",
	"type": "object",
	"allOf": [
		{
			"properties": {
				"format_version": {
					"anyOf": [
						{
							"$ref": "../project/formatVersionEnum.json"
						},
						{
							"const": "1.21.40"
						}
					]
				}
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": ["", "1.21.40"]
					}
				}
			},
			"then": {
				"$ref": "./v1.21.40/main.json"
			}
		}
	],
	"required": ["format_version", "minecraft:client_biome"]
}
