{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Volume",
	"type": "object",
	"required": ["format_version", "minecraft:volume"],
	"allOf": [
		{
			"properties": {
				"format_version": {
					"$ref": "../project/formatVersionEnum.json"
				}
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": [
							"",
							"1.8.0",
							"1.10.0",
							"1.11.0",
							"1.12.0",
							"1.13.0",
							"1.14.0",
							"1.16.0",
							"1.16.100",
							"1.16.200",
							"1.16.210",
							"1.16.220",
							"1.17.0",
							"1.17.10",
							"1.17.20"
						]
					}
				}
			},
			"then": {
				"$ref": "./v1.17.0/main.json"
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": ["1.17.30", "1.17.40", "1.18.0", "1.18.10"]
					}
				}
			},
			"then": {
				"$ref": "./v1.17.30/main.json"
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": [
							"1.18.20",
							"1.18.30",
							"1.19.0",
							"1.19.10",
							"1.19.20",
							"1.19.30"
						]
					}
				}
			},
			"then": {
				"$ref": "./v1.18.20/main.json"
			}
		}
	]
}
