{
	"$schema": "http://json-schema.org/draft-07/schema",
	"additionalProperties": false,
	"description": "Defines sets of items that can be used to trigger events when used on this entity. The item will also be taken and placed in the entity's inventory.",
	"type": "object",
	"title": "Giveable",
	"properties": {
		"triggers": {
			"title": "Triggers",
			"anyOf": [
				{
					"type": "array",
					"items": {
						"type": "object",
						"additionalProperties": false,
						"properties": {
							"cooldown": {
								"title": "Cooldown",
								"description": "An optional cool down in seconds to prevent spamming interactions.",
								"type": "number",
								"default": 0.0
							},
							"items": {
								"title": "Items",
								"description": "The list of items that can be given to the entity to place in their inventory.",
								"type": "array",
								"items": {
									"anyOf": [
										{
											"type": "string",
											"$ref": "../../../general/reference/identifiers.json#/definitions/item_identifiers"
										},
										{
											"type": "object",
											"additionalProperties": false,
											"properties": {
												"any_tag": {
													"title": "Any Tag",
													"type": "array",
													"items": {
														"type": "string",
														"$ref": "../../../general/reference/tags.json#/definitions/item_tags"
													}
												}
											}
										}
									]
								}
							},
							"on_give": {
								"title": "Event: On Give",
								"description": "Event to fire when the correct item is given.",
								"$ref": "../../general/v1.16.0/eventDefinition.json"
							}
						}
					}
				},
				{
					"type": "object",
					"properties": {
						"cooldown": {
							"title": "Cooldown",
							"description": "An optional cool down in seconds to prevent spamming interactions.",
							"type": "number",
							"default": 0.0
						},
						"items": {
							"title": "Items",
							"description": "The list of items that can be given to the entity to place in their inventory.",
							"type": "array",
							"items": {
								"anyOf": [
									{
										"type": "string",
										"$ref": "../../../general/reference/identifiers.json#/definitions/item_identifiers"
									},
									{
										"type": "object",
										"additionalProperties": false,
										"properties": {
											"any_tag": {
												"title": "Any Tag",
												"type": "array",
												"items": {
													"type": "string",
													"$ref": "../../../general/reference/tags.json#/definitions/item_tags"
												}
											}
										}
									}
								]
							}
						},
						"on_give": {
							"title": "Event: On Give",
							"description": "Event to fire when the correct item is given.",
							"$ref": "../../general/v1.16.0/eventDefinition.json"
						}
					}
				}
			]
		}
	}
}
