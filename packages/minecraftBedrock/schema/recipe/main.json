{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Recipe",
	"required": ["format_version"],
	"type": "object",
	"allOf": [
		{
			"properties": {
				"format_version": {
					"title": "Format Version",
					"type": "string",
					"anyOf": [
						{
							"$ref": "../project/formatVersionEnum.json"
						},
						{
							"const": "1.12"
						}
					]
				}
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": [
							"",
							"1.8.0",
							"1.10.0",
							"1.11.0",
							"1.12",
							"1.12.0",
							"1.13.0",
							"1.14.0",
							"1.16.0",
							"1.16.100",
							"1.16.200",
							"1.16.210",
							"1.16.220",
							"1.17.0",
							"1.17.10",
							"1.17.20",
							"1.17.30",
							"1.17.40",
							"1.18.0"
						]
					}
				}
			},
			"then": {
				"$ref": "./v1.16.0/main.json"
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": [
							"1.18.10",
							"1.18.20",
							"1.18.30",
							"1.19.0",
							"1.19.10",
							"1.19.20",
							"1.19.30"
						]
					}
				}
			},
			"then": {
				"$ref": "./v1.18.10/main.json"
			}
		},
		{
			"if": {
				"properties": {
					"format_version": {
						"enum": ["1.19.40"]
					}
				}
			},
			"then": {
				"$ref": "./v1.19.40/main.json"
			}
		}
	]
}
